						<div class="progress-pie-chart" data-percent="43">
							<div class="ppc-progress">
								<div class="ppc-progress-fill"></div>
							</div>
							<div class="ppc-percents">
								<div class="pcc-percents-wrapper">
									<span>%</span>
								</div>
							</div>

						</div>
						<a asp-controller="Profile" asp-action="Edit" asp-route-id="@Model.profileViewModel.UserId" class="btn btn-dark" style="margin-top: 12%;
    margin-left: 32%;">Lengkapi</a>
					</div>
					<div class="col-sm-7" style="background: #FFFFFF;
border: 2px solid #BDBDBD;
box-sizing: border-box;
border-radius: 5px;min-height: 700px;">


						<div class="row" style="margin: 10px auto;">
							<div class="col-sm-12">
								<h5 style="font-weight:bold; text-decoration:underline;">Data Diri</h5>

							</div>
						</div>
						<div class="row" style="margin: 10px auto;">
