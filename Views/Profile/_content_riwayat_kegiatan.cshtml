@model RegisterViewModel

<section class="mosh-service-area clearfix">
	<div class="container" style="margin-bottom:10%;">
		<div class="row" style="min-height: 700px;">
			@Html.Partial("_navigation_profile")

			<div class="col-sm-8" style="border: 1px solid grey; padding:10px;">
				<div class="row" style="background-color:rgba(144, 14, 14, 0.06);">
					<div class="col-sm-12 text-left">
						<h5 style="Font-Family: Roboto;
font-weight: bold;
Font-Size: 16px;
Line-Height: 19px;
text-align: Left;">
							Event
						</h5>
					</div>
				</div>
				<div class="row" style="margin-top:2%;">
					<div class="col-sm-12 text-center" id="EventContent">
						<input type="hidden" value="@Model.profileViewModel.aspnetusersId" id="idmember" />
						<div id="nodata">
							<h5>Anda belum mengikuti event DILo. Silahkan lihat event yang tersedia dengan mengklik tombol berikut</h5>
							<a asp-action="Index" asp-controller="Event" class="btn btn-dark">Lihat Event</a>
						</div>
						<div id="existdata">
							<table class="table table-bordered" style="border:1px solid black;">
								<thead>
									<tr>
										<th scope="col">Tanggal</th>
										<th scope="col">Nama Event</th>
										<th scope="col">Status Event</th>
										<th scope="col">Aksi</th>
									</tr>
								</thead>
								<tbody id="contentdata">
									
										@*<th scope="row">1</th>
										<td>Mark</td>
										<td>Otto</td>
										<td><a href="#" class="btn btn-dark">Detail</a></td>*@
									
								</tbody>
							</table>
						</div>
					</div>
				</div>
			</div>
		</div>
	</div>
</section>
<script src="~/lib/jquery/jquery.min.js"></script>
<script src="~/js/event_auth.js"></script>