							<div class="col-sm-6">
								<label for="password1">Tanggal Lahir &nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp; :</label>

							</div>
							<div class="col-sm-6">
								<label for="password1">@Model.profileViewModel.Birthday.ToString("dd/MM/yyy")</label>

							</div>
							<div class="col-sm-6">
								<label for="password1">No. Identitas &nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp; :</label>

							</div>
							<div class="col-sm-6">
								<label for="password1">@Model.profileViewModel.NoIdentitas</label>

							</div>
							<div class="col-sm-6">
								<label for="password1">Jenis Identitas &nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp; :</label>

							</div>
							<div class="col-sm-6">
								<label for="password1" style="text-transform:uppercase">@Model.profileViewModel.jenisIdentitas</label>

							</div>
							<div class="col-sm-6">
								<label for="password1">Lokasi   &nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp; :</label>

							</div>
							<div class="col-sm-6">
								<label for="password1" id="resdilolocation"></label>
								<input type="hidden" id="dilolocation" value="@Model.profileViewModel.dilolocation" />
							</div>
							@*<div class="col-sm-6">
			<label for="password1">Kota &nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp; :</label>

		</div>
		<div class="col-sm-6">
			<label for="password1">@Model.profileViewModel</label>

		</div>*@


						</div>
						<div class="row" style="margin: 10px auto;">
							<div class="col-sm-12">
								<h5 style="font-weight:bold; text-decoration:underline;">Media Sosial</h5>

							</div>
						</div>
