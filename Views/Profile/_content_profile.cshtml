							<div class="col-sm-6">
								<label for="password1">Linkedin &nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp; :</label>

							</div>
							<div class="col-sm-6">
								<label for="password1">@Model.profileViewModel.Linkedin</label>

							</div>
							<div class="col-sm-6">
								<label for="password1">Facebook &nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp; :</label>

							</div>
							<div class="col-sm-6">
								<label for="password1">@Model.profileViewModel.Facebook</label>

							</div>
							<div class="col-sm-6">
								<label for="password1">Google+ &nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp; :</label>

							</div>
							<div class="col-sm-6">
								<label for="password1">@Model.profileViewModel.Google</label>

							</div>
							<div class="col-sm-6">
								<label for="password1">Twitter &nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp; :</label>

							</div>
							<div class="col-sm-6">
								<label id="twitter_result"></label>
								<input type="hidden" id="twitter" value="@Model.profileViewModel.Twitter" />
							</div>
							<div class="col-sm-6">
								<label for="password1">Instagram &nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp; :</label>

							</div>
							<div class="col-sm-6">
								<label for="password1">@Model.profileViewModel.Instagram</label>

							</div>

						</div>



					</div>
				</div>
			</div>
		</div>
	</div>
</section>
<script type="text/javascript">
	var dilo = document.getElementById('dilolocation').value.replace(/([A-Z])/g, ' $1').trim();
	var result = document.getElementById('resdilolocation').innerHTML = dilo;
	var gender = document.getElementById('gender').value;
	var result_gender;
	if (gender === "male") {
		result_gender = "Laki-laki";

	} else if (gender === "female") {
		result_gender = "Perempuan";
	}
	document.getElementById('result_gender').innerHTML = result_gender;

	var twitter = document.getElementById('twitter').value;
	String.prototype.splice = function (idx, rem, str) {
		return this.slice(0, idx) + str + this.slice(idx + Math.abs(rem));
	};

	var result_twitter = twitter.splice(22, 0, "  ").splice(40, 0, "  ");

	console.log(result_twitter);
	document.getElementById('twitter_result').innerHTML = result_twitter; // "foo bar baz"

</script>